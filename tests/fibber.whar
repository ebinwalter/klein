fun main() -> int {
  int n;
  out <- "> ";
  in -> n;
  out <- "fib(";
  out <- n;
  out <- ") = ";
  out <- fib(n);
}

fun fib(int n) -> int {
  if n == 1 { return 1; }
  if n == 2 { return 1; }
  int a = 1;
  int b = 1;
  while n > 2 {
    int old = b;
    b = a + b;
    a = old;
    n = n - 1;
  }
  return b;
}
