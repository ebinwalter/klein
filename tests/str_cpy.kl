fun str_cpy(char *dest, char *src) {
  int ix = 0;
  while src[ix] != 0 {
    dst[ix] = src[ix];
    ix = ix + 1;
  }
}

fun strncpy(char *dest, char *src, int n) {
  int ix = 0;
  while src[ix] != 0 and ix < n - 1 {
    dest[ix] = src[ix];
    ix = ix + 1;
  }
  dest[ix] = '\0';
}

fun main() {
  char *snupis = "Snupis";
  char *usual = "usual";
  strcpy(snupis, usual);
}
